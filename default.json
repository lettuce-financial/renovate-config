{
  "dependencyDashboard": true,
  "extends": [
    "config:best-practices",
    "config:recommended",
    "group:nodeJs",
    "group:definitelyTyped",
    "group:jestMonorepo",
    "group:jestPlusTSJest",
    "group:jestPlusTypes"
  ],
  "ignoreDeps": [
    "kong",
    "pydantic-core",
    "pydantic_core",
    "starlette",
    "urllib3",
    "uvicorn",
    "@mui/x-data-grid",
    "@mui/x-data-grid-pro",
    "@mui/x-license"
  ],
  "minimumReleaseAge": "1 day",
  "packageRules": [
    {
      "matchPackageNames": [
	"lettuce-financial/actions"
      ],
      "minimumReleaseAge": "0 days"
    },
    {
      "matchPackageNames": ["react"],
      "allowedVersions": "<19.0.0"
    },
    {
      "matchPackageNames": ["react-dom"],
      "allowedVersions": "<19.0.0"
    },
    {
      "matchPackageNames": ["react-router-dom"],
      "allowedVersions": "<7.0.0"
    },
    {
      "matchPackageNames": ["twine"],
      "allowedVersions": "!/^5.1.0$/"
    },
    {
      "matchPackagePrefixes": [
	"kale"
      ],
      "minimumReleaseAge": "0 days"
    },
    {
      "matchPackageNames": [
	"boto3",
	"botocore"
      ],
      "groupName": "boto"
    },
    {
      "matchPackageNames": [
	"isort",
	"flake8-isort"
      ],
      "groupName": "isort"
    },
    {
      "matchPackageNames": [
	"fastapi",
	"starlette",
	"uvicorn"
      ],
      "groupName": "fastapi"
    },
    {
      "matchPackageNames": [
	"pydantic",
	"pydantic-core",
	"pydantic-settings"
      ],
      "groupName": "pydantic"
    },
    {
      "matchPackageNames": [
	"requests",
	"urllib3"
      ],
      "groupName": "requests"
    }
  ],
  "pyenv": {
    "enabled": false
  }
}
