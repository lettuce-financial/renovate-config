{
  "dependencyDashboard": true,
  "extends": [
    "config:best-practices",
    "config:recommended",
    "group:nodeJs",
    "group:definitelyTyped",
    "group:jestMonorepo",
    "group:jestPlusTSJest",
    "group:jestPlusTypes"
  ],
  "ignoreDeps": [
    "kong",
    "pydantic-core",
    "pydantic_core",
    "starlette",
    "urllib3",
    "uvicorn"
  ],
  "minimumReleaseAge": "1 day",
  "packageRules": [
    {
      "matchPackagePrefixes": [
	"kale"
      ],
      "minimumReleaseAge": "0 days"
    },
    {
      "matchPackageNames": [
	"boto3",
	"botocore"
      ],
      "groupName": "boto"
    },
    {
      "matchPackageNames": [
	"fastapi",
	"starlette",
	"uvicorn"
      ],
      "groupName": "fastapi"
    },
    {
      "matchPackageNames": [
	"pydantic",
	"pydantic-core",
	"pydantic-settings"
      ],
      "groupName": "pydantic"
    },
    {
      "matchPackageNames": [
	"requests",
	"urllib3"
      ],
      "groupName": "requests"
    }
  ],
  "pyenv": {
    "enabled": false
  }
}
